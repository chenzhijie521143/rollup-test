{
  "name": "rollup-test",
  "version": "1.0.0",
  "description": "rollup 学习",
  "main": "index.js",
  "scripts": {
    "dev": "npm run start",
    "start": "rm -rf example/dist && yarn && rollup -wc ./build/rollup.dev.js",
    "build": "rm -rf dist && rollup -c ./build/rollup.build.js",
    "build:babel": "rm -rf dist && yarn build:types && rollup -c ./build/rollup.build.babel.js",
    "build:types": "tsc -b ./tsconfig.types.json",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch"
  },
  "author": "chenzhijie",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-proposal-decorators": "^7.17.12",
    "@babel/preset-env": "^7.18.0",
    "@babel/preset-react": "^7.17.12",
    "@babel/preset-typescript": "^7.17.12",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "autoprefixer": "^10.4.7",
    "core-js": "2",
    "cssnano": "^5.1.9",
    "less": "^4.1.2",
    "postcss": "^8.4.14",
    "rollup": "^2.74.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  }
}
